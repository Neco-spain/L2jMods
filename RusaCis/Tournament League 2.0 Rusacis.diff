+			team.teleportBack();
+			team.backInstance();
+		}
+		
+	}
+	
+	public void finish()
+	{
+		try
+		{
+			if (getWinner() != null)
+			{
+				// reward victorious team
+				getWinner().reward(true);
+				// store +1 victory
+				getWinner().addTeamVictory(fightType);
+				
+				if (getLooser() != null)
+				{
+					// reward looser team
+					getLooser().reward(false);
+					getLooser().addTeamDefeat(fightType);
+					
+				}
+				
+			}
+			else
+			{
