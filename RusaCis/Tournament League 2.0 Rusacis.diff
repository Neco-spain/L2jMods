+		{
+			if (player.isInParty())
+			{
+				player.getParty().disband();
+			}
+		}
+		if (getMembers().size() > 1)
+		{
+			party = new Party(leader, getMembers().get(1), LootRule.ITEM_LOOTER);
+		}
+		for (Player player : getMembers())
+		{
+			if (party != null && !player.isInParty())
+			{
+				party.addPartyMember(player);
+			}
+			// revive dead players
+			if (player.isDead())
+			{
+				player.doRevive();
+			}
+			// heal players
